    .footer__wrapper
      .row
        .logo
          img(src="img/logo-black.png" alt="Misocial")
        .footer__menu
          +menu()(class="menu_theme_dark")
      .line
      .row
        p.footer__copy Â© 2017 Mass Impressions & Tranmautritam. Make the right impression.
        +social-links().footer__social-links
